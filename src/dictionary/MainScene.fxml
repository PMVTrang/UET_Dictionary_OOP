<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="400" minWidth="400"
            prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.MainSceneController">
   <children>
      <TabPane layoutX="1.0" layoutY="2.0"
               prefHeight="400.0" prefWidth="600.0"
               tabClosingPolicy="UNAVAILABLE"
               AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
         <tabs>
            <Tab fx:id="tabDictionary" text="Dictionary">
               <content>
                  <fx:include source="TabDictionary.fxml" />
               </content>
            </Tab>
            <Tab fx:id="tabTranslate" text="Translate">
               <content>
                  <fx:include source="TabTranslate.fxml" />
               </content>
               
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
